{
    "compilerOptions": {
        "noEmit": true,
        "allowJs": true,
        "checkJs": true,
        "target": "ESNext",
        "allowUnusedLabels": false,
        "allowUnreachableCode": false,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    },
    "include": [
        "*.*"
    ],
}